<% include header.ejs %>
 	<form method="post" class="form-horizontal">
		<div class="form-group">
			<label class="col-md-1 control-label">用户名</label>
			<div class="col-md-2">
				<input class="form-control col-md-7" type="text" name="name"/>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-1 control-label">密码</label>
			<div class="col-md-2">
				<input class="form-control col-md-7" type="text" name="password"/>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-1 control-label">类别（0：永久 1：一个月）</label>
			<div class="col-md-2">
				<input class="form-control col-md-7" type="text" name="type"/>
			</div>
		</div>
		<div class="col-md-2 col-md-offset-1">
			 <input type="submit"  class="btn btn-primary" value="注册"/>
		</div>
	        
	</form>
	<% if (success) { %>
	  <div><%= success %></div>
	<% } %>
	<% if (error) { %>
	  <div><%= error %> </div>
	<% } %>




<% include footer.ejs %>
